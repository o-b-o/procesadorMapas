package com.proyecto;

public abstract class FileHandlr{
    protected String caminoEntrada;
    protected String caminoSalida;
    public FileHandlr(String inPath, String outPath){
        this.caminoEntrada = inPath;                              
        this.caminoSalida = outPath;                              
    }

    
    public abstract ConjuntoDeMapas leerDatosCompletos();
}
